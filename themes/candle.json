{
  "name": "Candle Theme",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#1B1B1B",
        "foreground": "#E7E7E7"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#575757"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "name": "PHP Constants",
      "scope": [
